pronom.Lossiness:Lossless
  rdf:type pronom2020:Lossiness ;
  rdfs:comment "Lossless compression is a class of data compression algorithms that allows the original data to be perfectly reconstructed from the compressed data."@en ;
  rdfs:label "Lossless"@en ;
  skos:broader pronom2020:Lossiness ;
  skos:prefLabel "Lossless"@en ;
.
pronom.Lossiness:Lossy
  rdf:type pronom2020:Lossiness ;
  rdfs:comment "Lossy compression permits reconstruction only of an approximation of the original data, though usually with greatly improved compression rates (and therefore reduced media sizes)."@en ;
  rdfs:label "Lossy"@en ;
  skos:broader pronom2020:Lossiness ;
  skos:prefLabel "Lossy"@en ;
